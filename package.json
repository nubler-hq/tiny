{
  "name": "saas-boilerplate",
  "version": "1.0.1",
  "private": true,
  "overrides": {
    "@modelcontextprotocol/sdk": "npm:@socotra/modelcontextprotocol-sdk"
  },
  "scripts": {
    "dev": "next dev",
    "compile": "next experimental-compile",
    "dev:email": "email dev --dir src/content/mails --port 3001",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest --watch",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "db:studio": "prisma studio",
    "db:migrate:dev": "prisma migrate dev",
    "db:generate": "prisma generate",
    "auth:generate": "npx @better-auth/cli generate --config src/core/providers/auth.ts",
    "auth:migrate": "npx @better-auth/cli migrate --config src/core/providers/auth.ts",
    "stripe:webhook": "stripe listen --forward-to localhost:3000/api/billing/webhook",
    "validate:payment": "node scripts/validate-payment-config.js",
    "analyze-bundle": "ANALYZE=true npm run build",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish",
    "postinstall": "fumadocs-mdx"
  },
